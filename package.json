{
  "name": "lazy-formatter",
  "version": "0.0.0",
  "description": "Select the code formatter that makes the smallest file difference.",
  "keywords": [
    "code",
    "formatter",
    "lazy",
    "migration"
  ],
  "homepage": "https://github.com/hata6502/lazy-formatter#readme",
  "bugs": {
    "url": "https://github.com/hata6502/lazy-formatter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hata6502/lazy-formatter.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/hata6502"
  },
  "license": "MIT",
  "author": "Tomoyuki Hata",
  "bin": {
    "lazy-format": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fix": "prettier --write .",
    "test": "prettier --check . && tsc --noEmit"
  },
  "dependencies": {
    "diff": "^5.0.0",
    "execa": "^5.0.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/minimatch": "^3.0.4",
    "@types/node": "^14.14.37",
    "prettier": "^2.2.1",
    "semantic-release": "^21.0.1",
    "typescript": "^4.2.4"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
